<script>
    var elec;
    function multipleEventsListeners(elem, events, func) {
        var event = events.split(' ');
        for (var i = 0; i < event.length; i++) {
            elem.addEventListener(event[i], func, false);
        }
    }
    function selectImg(l){
        for (let i = 1; i <= l; i++) {
          e=document.getElementById('r'+i)
          multipleEventsListeners(e,'click', function(e){
            //window.alert('Elegiste: '+this.alt);
            divi=document.getElementById('div_'+i);
            divi.style='border: 5px solid #252850;';
            localStorage.setItem("Elegido1", this.alt);
            elec=document.getElementById('elegido');
            elec.value = this.alt;
            //elec.style='background-color:blue;';
            numOpci=document.getElementById('numOpci');
            numOpci.value = localStorage.getItem("NumOpc1");
            tiempo=document.getElementById('tiempo');
            var fin = new Date();
            tiempo.value = (fin-inicio)*0.001;
        })
        }
    }
    var inicio = new Date();
    selectImg(8);
</script>
